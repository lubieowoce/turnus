{{
    {
        events: page.children | map (p => {
            title: p.title,
            location: p.header.location,
            time: p.header.time,
            id: p.slug,
            summary: p.summary(500) | striptags,
            media: p.media | map(m => { url: m.url, meta: m.meta }),
        }),
    }
    | json_encode | raw
}}
